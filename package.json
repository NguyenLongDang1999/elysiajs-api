    "drizzle-orm": "^0.31.4",
    "drizzle-typebox": "^0.1.1",
    "elysia": "latest",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.6",
    "bun-types": "latest",
    "drizzle-kit": "^0.22.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  },
  "module": "src/index.js"
